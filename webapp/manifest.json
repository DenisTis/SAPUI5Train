{
  "_version": "0.0.1",
  "sap.app": {
    "id": "companyRepo.appName",
    "type": "application",
    "dataSources": {
      "backendService": {
        "uri": "/sap/opu/odata/backendServicePath",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.76.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.f": {}
      }
    },
    "rootView": {
      "viewName": "companyRepo.appName.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "companyRepo.appName.i18n.i18n",
          "supportedLocales": ["en", "de"],
          "fallbackLocale": "en"
        }
      }
    },
    "resources": {
      "css": [{
        "uri": "css/styles.css"
      }]
    }
  }
}
